{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon ./bin/server",
    "watch": "DEBUG=exspender:* concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "start": "node ./bin/server.js"
  },
  "dependencies": {
    "@types/mongoose": "^5.11.97",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "express-validator": "^6.13.0",
    "mongoose": "^6.0.10",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/node": "^16.10.3",
    "concurrently": "^6.3.0",
    "nodemon": "^2.0.13",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  }
}